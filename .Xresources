Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!Xft.hintstyle: hintfull


Xcursor.theme: xcursor-breeze-snow
Xcursor.size:                     0


!-------------------------------------------------------------------------------
! URxvt settings
!-------------------------------------------------------------------------------

URxvt.perl-ext-common:      resize-font,eval

URxvt*depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       true
URxvt*saveLines:                  2000
URxvt.internalBorder:  0

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard

!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

! Fonts
URxvt*font:xft:DejaVu Sans Mono for Powerline:size=14:antialias=true:hinting=true, xft:FiraCode Nerd Font Mono:size=14, xft:devicons:size=14
URxvt.boldFont: xft:DejaVu Sans Mono:bold:size=14:antialias=true:hinting=true, xft:FiraCode Nerd Font Mono:size=14
URxvt.intensityStyles: false

! Fix font space
URxvt.letterSpace: -1

! Scrollbar
URxvt.scrollStyle:          plain
URxvt.matcher.button:       1
URxvt.urlLauncher:          firefox-developer-edition

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      true

! Pointer
URxvt.pointerBlank:         true

! ------------------------------------------------------------------------------
! Rofi configs
! ------------------------------------------------------------------------------

rofi.color-enabled: true
rofi.color-window: argb:e92e3440, #2e3440, argb:ff2e3440
rofi.color-normal: argb:002e3440, #f2f4f7, argb:002e3440, argb:002e3440, #bf616a
rofi.color-active: argb:002e3440, #b48ead, argb:e92e3440, argb:e92e3440, #93e5cc
rofi.color-urgent: argb:002e3440, #ebcb8b, argb:e92e3440, argb:e92e3440, #ebcb8b
rofi.modi: run,drun,window
!rofi.color-window: argb:dd2f343f, #002b36, #292929
!rofi.color-normal: argb:002f343f, #ffffff, argb:002f343f, argb:002f343f, #19e4b0
!rofi.color-active: argb:002f343f, #ffffff, argb:002f343f, argb:002f343f, #ffffff
!rofi.color-urgent: argb:002f343f, #ff0000, argb:002f343f, argb:002f343f, #ff0000
rofi.font: 		DejaVu Sans Mono 14
rofi.columns: 		2
rofi.hide-scrollbar: 	true
rofi.width: 		60
rofi.lines: 		12
rofi.padding: 		18
rofi.sidebar-mode: 	true
rofi.fuzzy: 		true
rofi.separator-style: 	none

! Switch to the next mode.
rofi.kb-mode-next:                   Shift+Right,Control+Tab,Control+l
! Switch to the previous mode.
rofi.kb-mode-previous:               Shift+Left,Control+Shift+Tab,Control+h

rofi.kb-row-up:                      Up,Control+k,Shift+Tab
rofi.kb-row-down:                    Down,Control+j

! Delete previous char
rofi.kb-remove-char-back:            BackSpace

rofi.matching: 			     fuzzy
!rofi.sorting-method:	    	     fzf

rofi.kb-accept-entry: 	 	     Return
rofi.kb-remove-to-eol:


